# ---------------------------------------------------------------------------
!if !$d(BCB)
BCB = $(MAKEDIR)\..
!endif

# ---------------------------------------------------------------------------
# IDE SECTION
# ---------------------------------------------------------------------------
# The following section of the project makefile is managed by the BCB IDE.
# It is recommended to use the IDE to change any of the values in this
# section.
# ---------------------------------------------------------------------------

VERSION = BCB.04.04
# ---------------------------------------------------------------------------
PROJECT = prn2sword.exe
OBJFILES = prn2sword.obj prn2swordfrm.obj ..\src\keys\versekey.obj ..\src\keys\strkey.obj \
  ..\src\keys\swkey.obj ..\src\keys\listkey.obj ..\src\utilfuns\utilstr.obj
RESFILES = prn2sword.res
RESDEPEN = $(RESFILES) prn2swordfrm.dfm
LIBFILES =
IDLGENFILES =
IDLFILES =
LIBRARIES = Vcl40.lib
SPARELIBS = Vcl40.lib
PACKAGES = Vcl40.bpi Vclx40.bpi vcljpg40.bpi Vclmid40.bpi Vcldb40.bpi bcbsmp40.bpi \
  vcldbx40.bpi Qrpt40.bpi TeeUI40.bpi teedb40.bpi tee40.bpi Dss40.bpi ibsmp40.bpi \
  NMFast40.bpi Inetdb40.bpi Inet40.bpi dclocx40.bpi dclusr40.bpi
DEFFILE =
# ---------------------------------------------------------------------------
PATHCPP = .;..\src\keys;..\src\utilfuns
PATHASM = .;
PATHPAS = .;
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
USERDEFINES =
SYSDEFINES = NO_STRICT
# ---------------------------------------------------------------------------
CFLAG1 = -I..\src\utilfuns;..\src\keys;$(BCB)\include;$(BCB)\include\vcl;..\include -O2 \
  -Hc -H=$(BCB)\lib\vcl40.csm -w -Ve -a8 -k- -vi -c -b- -w-par -w-inl -Vx -tW \
  -tWM -D$(SYSDEFINES);$(USERDEFINES)
IDLCFLAGS = -I..\src\utilfuns -I..\src\keys -I$(BCB)\include -I$(BCB)\include\vcl \
  -I..\include -src_suffixcpp
PFLAGS = -U..\src\utilfuns;..\src\keys;$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -I..\src\utilfuns;..\src\keys;$(BCB)\include;$(BCB)\include\vcl;..\include -$Y- \
  -$L- -$D- -v -JPHNE -M
RFLAGS = -i..\src\utilfuns;..\src\keys;$(BCB)\include;$(BCB)\include\vcl;..\include
AFLAGS = /i..\src\utilfuns /i..\src\keys /i$(BCB)\include /i$(BCB)\include\vcl \
  /i..\include /mx /w2 /zn
LFLAGS = -L..\src\utilfuns;..\src\keys;$(BCB)\Projects\Lib;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -aa -Tpe -x -Gn
# ---------------------------------------------------------------------------
ALLOBJ = c0w32.obj sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mt.lib
# ---------------------------------------------------------------------------
